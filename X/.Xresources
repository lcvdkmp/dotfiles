! special
! *.foreground:   #f8f6f5
! *.background:   #231c17
*.cursorColor:  #c8c6c5
*.background:   #fdf5e6
*.foreground:   #002211

! black
*.color0:       #231c17
*.color8:       #5a5451

! red
*.color1:       #e31a1c
*.color9:       #ce463e

! green
*.color2:       #32c880
*.color10:      #2bb076

! yellow
*.color3:       #d59c57
*.color11:      #d59c57

! blue
*.color4:       #2c6ab1
*.color12:      #3182bd

! magenta
*.color5:       #756bb1
*.color13:      #756bb1

! cyan
