URxvt.scrollBar: false
!Xft.dpi:                     142
!urxvt.font: xft:DejaVu Sans Mono for Powerline:size=10,xft:DejaVu Sans:size=10
!urxvt.font: xft:Input Mono:size=11:hinting=true,xft:Consolas:size=13:hinting=true,xft:DejaVu Sans:size=10:hinting=true
urxvt.font: xft:Input Mono:size=11
*antialias:  true
! *autohint:   true
xft.hinting: true
xft.hintstyle: hintslight
! xft.hintstyle: hintfull
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb
URxvt.letterSpace: -1

URxvt.perl-ext-common: default,matcher,clipboard,resize-font,tabbedex
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.clipboard.autocopy: true
URxvt.keysym.C-M-c: perl:clipboard:copy
URxvt.keysym.C-M-v: perl:clipboard:paste

URxvt.resize-font.step: 1
URxvt.keysym.C-M-minus:     resize-font:smaller
URxvt.keysym.C-M-plus:      resize-font:bigger
URxvt.keysym.C-M-equal:     resize-font:reset
URxvt.keysym.C-M-question:  resize-font:show

! URxvt.tabbedex.no-tabbedex-keys: yes
! URxvt.keysym.Shift-Left:    tabbedex:prev_tab
! URxvt.keysym.Shift-Right:   tabbedex:next_tab
! URxvt.keysym.Shift-Down:    tabbedex:new_tab
! URxvt.keysym.Shift-Up:      tabbedex:rename_tab
! URxvt.keysym.Control-Left:  tabbedex:move_tab_left
! URxvt.keysym.Control-Right: tabbedex:move_tab_right
! URxvt.keysym.Meta-F1:       tabbedex:goto_tab_1
! URxvt.keysym.Meta-F2:       tabbedex:goto_tab_2
! URxvt.keysym.Meta-F3:       tabbedex:goto_tab_3
! URxvt.keysym.Meta-F4:       tabbedex:goto_tab_4
! URxvt.keysym.Meta-F5:       tabbedex:goto_tab_5
! URxvt.keysym.Meta-F6:       tabbedex:goto_tab_6
! URxvt.keysym.Meta-F7:       tabbedex:goto_tab_7
! URxvt.keysym.Meta-F8:       tabbedex:goto_tab_8
! URxvt.keysym.Meta-F9:       tabbedex:goto_tab_9
! URxvt.keysym.Meta-F10:      tabbedex:goto_tab_10
! URxvt.keysym.Meta-F11:      tabbedex:goto_tab_11
! URxvt.keysym.Meta-F12:      tabbedex:goto_tab_12

!rofi.eh: 3
!rofi.width: 100
!rofi.padding: 300
rofi.lines: 8
rofi.columns: 4
rofi.line-margin: 0
rofi.line-padding: 1
rofi.hide_scrollbar: True
rofi.scrollbar_width: 0
rofi.terminal: urxvt
!rofi.location: 1
